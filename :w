var quicksortDupModule = module.exports = {

  duplicates: [],

  sort: function sortArray(someArray) {
    someArray.sort(function(a, b) {
      if (a > b) {
        return 1;
      }
      if (b > a) {
        return -1;
      }
      var dupes = this.duplicates;
      dupes.push(a);
      return 0;
    });
  }
};
